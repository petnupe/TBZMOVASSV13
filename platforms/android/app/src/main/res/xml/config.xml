<?xml version='1.0' encoding='utf-8'?>
<widget codeVersion="314" id="com.tecbiz.tecbizassociado" version="314" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Notification">
        <param name="android-package" value="org.apache.cordova.dialogs.Notification" />
    </feature>
    <feature name="FCMPlugin">
        <param name="android-package" value="com.gae.scaffolder.plugin.FCMPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="NetworkStatus">
        <param name="android-package" value="org.apache.cordova.networkinformation.NetworkManager" />
    </feature>
    <name>TecBiz Associado</name>
    <description>
        Aplicativo para consulta de saldos, extratos e lojas credenciadas.
    </description>
    <author email="tecnologia@tecbiz.com.br" href="http://www.tecbiz.com.br">
        TecBiz Tecnologia e negocios
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon qualifier="ldpi" src="www/res/icons/android/icon-36-ldpi.png" />
    <icon qualifier="mdpi" src="www/res/icons/android/icon-48-mdpi.png" />
    <icon qualifier="hdpi" src="www/res/icons/android/icon-72-hdpi.png" />
    <icon qualifier="xhdpi" src="www/res/icons/android/icon-96-xhdpi.png" />
    <icon qualifier="xxhdpi" src="www/res/icons/android/logo144x144.png" />
    <icon qualifier="xxxhdpi" src="www/res/icons/android/logo192x192.png" />
    <splash qualifier="port-ldpi" src="www/res/screens/android/screen240x320.png" />
    <splash qualifier="land-ldpi" src="www/res/screens/android/screen320x240.png" />
    <splash qualifier="port-mdpi" src="www/res/screens/android/screen-mdpi-portrait.png" />
    <splash qualifier="land-mdpi" src="www/res/screens/android/screen-mdpi-landscape.png" />
    <splash qualifier="port-hdpi" src="www/res/screens/android/screen-hdpi-portrait.png" />
    <splash qualifier="land-hdpi" src="www/res/screens/android/screen-hdpi-landscape.png" />
    <splash qualifier="port-xhdpi" src="www/res/screens/android/screen-xhdpi-portrait.png" />
    <splash qualifier="land-xhdpi" src="www/res/screens/android/screen-xhdpi-landscape.png" />
    <splash qualifier="port-xxhdpi" src="www/res/screens/android/screen960x1600.png" />
    <splash qualifier="land-xxhdpi" src="www/res/screens/android/screen1600x960.png" />
    <splash qualifier="port-xxxhdpi" src="www/res/screens/android/screen1280x1920.png" />
    <splash qualifier="land-xxxhdpi" src="www/res/screens/android/screen1920x1280.png" />
    <resource-file src="./google-services.json" target="./google-services.json" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="phonegap-version" value="cli-7.0.1" />
</widget>
